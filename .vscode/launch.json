{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "factoriomod",
      "request": "launch",
      "name": "Test run",
      "adjustMods": {
        "bp100": true,
        "WiderNoticeTextBoxes": true,
        "factorio-test": true,
        "PickerDollies": true,
        "stdlib": true
      },
      "disableExtraMods": true,
      "factorioArgs": [
        "--load-scenario",
        "factorio-test/Test",
        "--cache-sprite-atlas",
        "true",
        "--graphics-quality",
        "low",
        "-c",
        "${workspaceRoot}/factorio-test-data/config.ini"
      ],
      "modsPath": "${workspaceRoot}/factorio-test-data/mods",
      "checkGlobals": false,
      "hookControl": false,
      "adjustModSettings": [
        {
          "scope": "startup",
          "name": "factorio-test-auto-start",
          "value": true
        }, {
          "scope": "runtime-global",
          "name": "factorio-test-mod-to-test",
          "value": "bp100"
        }
      ]
    }, {
      "type": "factoriomod",
      "request": "launch",
      "name": "Profile",
      "adjustMods": {
        "bp100": true,
        "WiderNoticeTextBoxes": true,
        "factorio-test": false,
        "PickerDollies": true,
        "stdlib": true
      },
      "disableExtraMods": true,
      "factorioArgs": [
        "--load-scenario",
        "factorio-test/Test",
        "--cache-sprite-atlas",
        "true",
        "--graphics-quality",
        "low",
        "-c",
        "${workspaceRoot}/factorio-test-data/config.ini"
      ],
      "modsPath": "${workspaceRoot}/factorio-test-data/mods",
      "checkGlobals": false,
      "hookControl": true,
      "hookMode": "profile"
    }, {
      "type": "factoriomod",
      "request": "launch",
      "name": "Test debug",
      "adjustMods": {
        "bp100": true,
        "WiderNoticeTextBoxes": true,
        "factorio-test": true,
        "PickerDollies": true,
        "stdlib": true
      },
      "disableExtraMods": true,
      "factorioArgs": [
        "--load-scenario",
        "factorio-test/Test",
        "--cache-sprite-atlas",
        "true",
        "--graphics-quality",
        "low",
        "-c",
        "${workspaceRoot}/factorio-test-data/config.ini"
      ],
      "modsPath": "${workspaceRoot}/factorio-test-data/mods",
      "checkGlobals": false,
      "hookControl": true
    }, {
      "type": "factoriomod",
      "request": "launch",
      "name": "debug data",
      "adjustMods": {
        "bp100": true,
        "WiderNoticeTextBoxes": true,
        "factorio-test": true
      },
      "disableExtraMods": true,
      "factorioArgs": [
        "--load-scenario",
        "factorio-test/Test",
        "--cache-sprite-atlas",
        "true",
        "--graphics-quality",
        "low",
        "-c",
        "${workspaceRoot}/factorio-test-data/config.ini"
      ],
      "modsPath": "${workspaceRoot}/factorio-test-data/mods",
      "checkGlobals": false,
      "hookData": true,
      "hookControl": false
    }, {
      "type": "factoriomod",
      "request": "launch",
      "name": "debug only",
      "factorioArgs": [
        "--cache-sprite-atlas", "true", "--graphics-quality", "low"
      ],
      "checkGlobals": false,
      "hookControl": true
    }
  ]
}
