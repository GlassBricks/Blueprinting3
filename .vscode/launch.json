{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "factoriomod",
      "request": "launch",
      "name": "Test run",
      "adjustMods": {
        "bp100": true,
        "WiderNoticeTextBoxes": true,
        "testorio": true,
        "PickerDollies": true,
        "stdlib": true
      },
      "disableExtraMods": true,
      "factorioArgs": [
        "--load-scenario",
        "bp100Test",
        "--cache-sprite-atlas",
        "true",
        "--graphics-quality",
        "low",
        "-c",
        "${workspaceRoot}/factorio/config.ini"
      ],
      "modsPath": "${workspaceRoot}/factorio",
      "checkGlobals": false,
      "hookControl": false
    },
    {
      "type": "factoriomod",
      "request": "launch",
      "name": "Test debug",
      "adjustMods": {
        "bp100": true,
        "WiderNoticeTextBoxes": true,
        "testorio": true,
        "PickerDollies": true,
        "stdlib": true
      },
      "disableExtraMods": true,
      "factorioArgs": [
        "--load-scenario",
        "bp100Test",
        "--cache-sprite-atlas",
        "true",
        "--graphics-quality",
        "low",
        "-c",
        "${workspaceRoot}/factorio/config.ini"
      ],
      "modsPath": "${workspaceRoot}/factorio",
      "checkGlobals": false,
      "hookControl": true
    },
    {
      "type": "factoriomod",
      "request": "launch",
      "name": "debug data",
      "adjustMods": {
        "bp100": true,
        "WiderNoticeTextBoxes": true,
        "testorio": true
      },
      "disableExtraMods": true,
      "factorioArgs": [
        "--load-scenario",
        "bp100Test",
        "--cache-sprite-atlas",
        "true",
        "--graphics-quality",
        "low",
        "-c",
        "${workspaceRoot}/factorio/config.ini",
      ],
      "modsPath": "${workspaceRoot}/factorio",
      "checkGlobals": false,
      "hookData": true,
      "hookControl": false
    },
    {
      "type": "factoriomod",
      "request": "launch",
      "name": "debug only",
      "factorioArgs": ["--cache-sprite-atlas", "true", "--graphics-quality", "low"],
      "checkGlobals": false,
      "hookControl": true
    }
  ]
}
