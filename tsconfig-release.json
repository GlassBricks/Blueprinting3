{
  "extends": "./tsconfig.json",
  "exclude": ["**/*.test.{ts,tsx}", "src/test.ts", "src/test-util", "**/*.mock.{ts,tsx}"],
  "tstl": {
    "luaTarget": "5.2",
    "noResolvePaths": ["__testorio__/init"],
    "noImplicitSelf": true,
    "sourceMapTraceback": true,
    "luaPlugins": [
      {
        "name": "./tstlPlugin/plugin.js"
      }
    ]
  }
}
