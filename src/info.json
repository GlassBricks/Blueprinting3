{
  "$schema": "https://raw.githubusercontent.com/justarandomgeek/vscode-factoriomod-debug/e7dc1da746fe56f6d5202be6da953f265b2dbeeb/schema/modinfo.json",
  "name": "bp100",
  "version": "0.28.0",
  "title": "Staged Blueprint Planning",
  "description": "The ultimate mod for designing staged blueprints.",
  "author": "GlassBricks",
  "factorio_version": "1.1",
  "package": {
    "readme": "mod-portal/readme.md",
    "gallery": [
      "mod-portal/gallery/Highlights.png",
      "mod-portal/gallery/Entity-info.png",
      "mod-portal/gallery/A0.png",
      "mod-portal/gallery/A1.png",
      "mod-portal/gallery/A2.png",
      "mod-portal/gallery/A3.png"
    ],
    "scripts": {
      "compile": "npm run build:release",
      "prepublish": "npm run check"
    },
    "git_publish_branch": "main",
    "ignore": ["readme.md", "**/test/", "**/*.xcf", "**/*.svg", "mod-portal/**/*"],
    "removeZipAfterPackage": true,
    "no_git_tag": true,
    "sync_portal_details": true
  },
  "dependencies": ["base >= 1.1.71", "? PickerDollies >= 1.2.1", "? bobinserters >= 1.0.0", "? boblogistics >= 1.0.0"]
}
