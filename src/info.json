{
  "name": "bp100",
  "version": "0.27.0",
  "title": "Staged Blueprint Planning",
  "description": "The ultimate mod for designing staged blueprints, e.g. for 100% speedruns.",
  "author": "GlassBricks",
  "factorio_version": "1.1",
  "package": {
    "readme": "../mod-portal/readme.md",
    "gallery": [
      "../mod-portal/gallery/Assembly info.png",
      "../mod-portal/gallery/Highlights.png",
      "../mod-portal/gallery/s1.png",
      "../mod-portal/gallery/s2.png",
      "../mod-portal/gallery/s3.png"
    ],
    "scripts": {
      "compile": "npm run build:release",
      "prepublish": "npm run check"
    },
    "git_publish_branch": "main",
    "ignore": ["readme.md", "test/**/*", "**/*.xcf", "**/*.svg"],
    "removeZipAfterPackage": true,
    "no_git_tag": true
  },
  "dependencies": [
    "base >= 1.1.71", "? PickerDollies >= 1.2.1", "? bobinserters >= 1.0.0", "? boblogistics >= 1.0.0"
  ]
}
